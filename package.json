{
  "name": "@devoinc/genesys-ui-form-builder",
  "version": "0.2.1",
  "description": "A form builder based on a descriptive field model for Genesys UI ecosystem ",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DevoInc/genesys-ui-form-builder.git"
  },
  "license": "MIT",
  "author": "Devo, Inc. Quvis Team (https://github.com/orgs/DevoInc/teams/quvis)",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.js",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cjs",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "build-storybook": "storybook build",
    "clean": "rm -rf coverage dist node_modules/.cache/storybook",
    "lint": "oxlint -c oxlintrc.json",
    "release": "release-it --dry-run",
    "release:ci": "release-it --ci",
    "start": "npm run storybook",
    "storybook": "storybook dev -p 6006",
    "stryker": "npx stryker run",
    "test": "vitest"
  },
  "devDependencies": {
    "@devoinc/genesys-brand-devo": "8.x.x",
    "@release-it/conventional-changelog": "^9.0.4",
    "@storybook/addon-a11y": "^8.4.7",
    "@storybook/addon-essentials": "^8.4.7",
    "@storybook/addon-themes": "^8.4.7",
    "@storybook/react": "^8.4.7",
    "@storybook/react-vite": "^8.4.7",
    "@stryker-mutator/core": "^8.7.1",
    "@stryker-mutator/vitest-runner": "^8.7.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^2.1.8",
    "happy-dom": "^16.0.1",
    "oxlint": "^0.15.3",
    "prettier": "^3.4.2",
    "release-it": "^17.11.0",
    "sass": "^1.83.0",
    "storybook": "^8.4.7",
    "terser": "^5.37.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.8"
  },
  "peerDependencies": {
    "@devoinc/genesys-base-styles": "2.x",
    "@devoinc/genesys-brand-devo": "8.x",
    "@devoinc/genesys-icons": "6.x",
    "@devoinc/genesys-ui": "15.x",
    "@devoinc/genesys-ui-color": "15.x",
    "@devoinc/genesys-ui-datetime": "15.x",
    "@dnd-kit/core": ">=6.0",
    "@dnd-kit/modifiers": ">=8.0",
    "@dnd-kit/sortable": ">=9.0",
    "@dnd-kit/utilities": ">=3.0",
    "react": ">=16.3",
    "react-dom": ">=16.3",
    "styled-components": ">=5.0"
  },
  "overrides": {
    "ahooks": {
      "react": "$react"
    },
    "react-dock": {
      "react": "$react",
      "@types/react": "$@types/react"
    },
    "react-grid-system": {
      "react": "$react"
    },
    "react-popper": {
      "react": "$react",
      "react-dom": "$react-dom"
    },
    "react-windowed-select": {
      "react": "$react",
      "react-dom": "$react-dom"
    }
  },
  "engines": {
    "node": ">=18",
    "npm": ">=8"
  },
  "publishConfig": {
    "access": "public"
  }
}
